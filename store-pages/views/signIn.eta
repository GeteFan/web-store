<% layout("./layouts/layout.eta") %>

<div class="phone-case-page-main-sign">     
    <div style="width:660px;margin:20px auto">
        <div>
            
            <div id="fs24">Log In</div>


            <form method="post" action="/contactus/submit/" class="validate payform" style="width:350px;padding:0 30px 50px;" novalidate="novalidate">
                <ul>
                    <li><input class="input-sign" name="contactUs.Name" type="text" required="" placeholder="Login" style="text-align:left;"></li>
                    <li><input class="input-sign" name="contactUs.Email" type="password" required="" data-rule-email="true" placeholder="Password" style="text-align:left;"></li>
                </ul>
                <button class="btn_4" type="button" style="width:200px" onclick="check()">Log In</button>
            </form>
        </div>
    </div>
</div>