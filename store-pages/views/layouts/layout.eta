            .header{
            background-color: #C1EFFF;
            width: 100%; 
                height: 47px;
                position: absolute; 
                top: 0; 
                left: 0; 
            
            }
            .wrap {
		    height: 47px;
            display: flex;
            justify-content: space-between;
              flex-direction: row;
            width:100%;
            overflow: hidden;
    
            }
            .logo {
              flex-direction: row;
            display: flex;
            height: 47px;
            width: 100px;
              
            
            }
            .logo-text{
        
            padding: 10px;
            font-size: 28px;
            color: black;
            text-align: center;
            
            }
            .logo-text a{
                    color: Black;
                    font-family: Arial, Helvetica, sans-serif;
                        font-weight: bold;
                    text-decoration: none;
            }
            .nav{
             display: block;
             text-allign: center;
              overflow: hidden;
            }
            ul {
            display: block;
            list-style-type: none;
            overflow: hidden;
            
            }
            .nav ul li {
            float: left;
            font-family: Arial, Helvetica, sans-serif;
            font-weight: bold;
            }


            .nav ul li a {
            display: block;
            color: black;
            text-align: center;
            padding: 14px 16px;
            text-decoration: none;
            }

            .nav ul li a:hover {
            background-color: #F1F1F1;
            text-decoration: underline;
            }

            .nav ul li:last-child{
            float:right;
            padding-top: 23px;
            padding-right: 12px;
            }
            .sign-up{
            display: block;
            display: flex;
            margin: 23px;
            height: 40px;
            width: 100px;
            }
            button {
            border: 2px solid grey; 
            color: black;
            padding: 5px 20px;
            text-align: center;
            text-decoration: none;
            display: inline-block;
            font-size: 16px;
            cursor: pointer;
            border-radius: 12px;
                margin: 0;
            -ms-transform: translate(-50%, -50%);
            transform: translate(-50%, -50%);
            
            }

            button:hover{
            border: 2px solid white; 
            background-color:black;
            color: white;
                border: black;
            
            }

            .tagline{
            padding-top: 40px;
                padding-left: 100px;
            font-size: 35px;
            width: 600px;
            float:left;
            }

            .desc{
            width: 600px;
            float: left;
            padding-left: 100px;
            font: 15px "Fira Sans", sans-serif;
            }
            .image{
            width: 300px;
            margin-left: 100px;
            margin-top: 90px;
            }
            .content{
            display: inline;
            }

